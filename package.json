{
  "name": "cabal-desktop",
  "version": "5.0.4",
  "description": "Cabal p2p offline-first desktop application",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "start:electron": "electron .",
    "start:webpack": "webpack",
    "start:nix": "nix run nixpkgs.electron_6 -c electron .",
    "start:webkit2": "webkit2-launcher .",
    "watch": "webpack --watch",
    "start": "cross-env NODE_ENV=development npm-run-all --parallel start:*",
    "pack": "electron-builder --dir",
    "dist": "yarn run build && electron-builder",
    "dist:multi": "electron-builder -mlw",
    "postinstall": "electron-builder install-app-deps",
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 jest --passWithNoTests"
  },

  "productName": "Cabal",
  "repository": "cabal-club/cabal-desktop",
  "author": {
    "name": "Cabal Club",
    "email": "github-noreply@cabal.club"
  },

  "launcher": {
    "title": "Cabal Desktop",
    "icon": "",
    "main": "./index.html",
    "width": 800,
    "height": 600
  },

  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "electron": "7.1.13",
    "electron-builder": "^22.5.1",
    "electron-notarize": "^0.3.0",
    "electron-rebuild": "^1.10.1",
    "jest": "^25.4.0",
    "node-sass": "^4.14.0",
    "npm-run-all": "^4.1.5",
    "prettier-standard": "^16.3.0",
    "sass-loader": "^8.0.2",
    "standard": "^14.3.3",
    "style-loader": "^1.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@reduxjs/toolkit": "^1.3.5",
    "babel-runtime": "^6.26.0",
    "cabal-client": "^4.1.0",
    "collect-stream": "^1.2.1",
    "dat-encoding": "^5.0.1",
    "debug": "^4.1.1",
    "del": "^5.1.0",
    "electron-default-menu": "^1.0.1",
    "electron-prompt": "^1.5.1",
    "electron-reload": "^1.5.0",
    "electron-window-state": "^5.0.3",
    "emoji-mart": "^3.0.0",
    "get-form-data": "^3.0.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "mkdirp": "^1.0.4",
    "moment": "^2.24.0",
    "mousetrap": "^1.6.5",
    "ms": "^2.1.2",
    "react": "^16.13.1",
    "react-blockies": "^1.4.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "remark": "^12.0.0",
    "remark-emoji": "^2.1.0",
    "remark-react": "^7.0.1",
    "strftime": "^0.10.0",
    "to2": "^1.0.0"
  },
  "build": {
    "appId": "club.cabal.desktop",
    "afterSign": "scripts/notarize.js",
    "productName": "Cabal",
    "protocols": [
      {
        "name": "cabal",
        "schemes": [
          "cabal"
        ]
      }
    ],
    "mac": {
      "category": "public.app-category.utilities",
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "gatekeeperAssess": false,
      "hardenedRuntime": true
    },
    "dmg": {
      "sign": false,
      "background": "static/images/dmg-background.tiff",
      "contents": [
        {
          "x": 135,
          "y": 200
        },
        {
          "x": 405,
          "y": 200,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "artifactName": "cabal-desktop-${version}-mac.${ext}"
    },
    "linux": {
      "target": [
        "AppImage",
        "snap",
        "deb"
      ],
      "category": "Network"
    },
    "appImage": {
      "artifactName": "cabal-desktop-${version}-linux-${arch}.${ext}"
    },
    "win": {
      "publisherName": "cabal"
    },
    "nsis": {
      "artifactName": "cabal-desktop-${version}-windows.${ext}"
    }
  },
  "np": {
    "yarn": false,
    "private": true
  }
}
